<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../master/head'); %>
        <title>Dashboard</title>
</head>

<body>

    <div class="wrapper fixed-top d-flex align-items-center">
        <!--Top menu -->
        <%- include('../master/header'); %>

            <!-- <header id="header" class="header fixed-top d-flex align-items-center">
        </header> -->
            <!-- sidebar -->
            <div class="sidebar">
                <!-- Sidebar.ejs -->
                <%- include('../master/sidebar'); %>
            </div>
    </div>
    <main id="main" class="main">
        <div class="pagetitle">
            <h1>Email</h1>
            <nav>
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="/">Home</a></li>
                    <li class="breadcrumb-item active">Report</li>
                </ol>
            </nav>
        </div><!-- End Page Title -->

        <section class="section">
            <div class="row">
                <div class="col-lg-12">

                    <div class="card">
                        <div class="card-body">
                            <br>
                            <table class="table table-borderless datatable" id="tableReport">
                                <thead>
                                    <tr>
                                        <!-- <th scope="col">No</th> -->
                                        <th scope="col">Driver</th>
                                        <th scope="col">PIC</th>
                                        <th scope="col">Berat</th>
                                        <th scope="col">Status</th>
                                        <th scope="col">Tanggal</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% data.forEach(function(data){ %>
                                        <tr>
                                            <!-- <th scope="row"><%= data.id %></th> -->
                                            <td>
                                                <%= data.driver %>
                                            </td>
                                            <td>
                                                <%= data.pic %>
                                            </td>
                                            <td>
                                                <%= data.berat %>
                                            </td>
                                            <td><span class="badge bg-success"><%= data.status_email %></span></td>
                                            <td>
                                                <%= data.tanggal %>
                                            </td>
                                            <!-- <td><span class="badge bg-success">21-04-2022</span></td> -->
                                        </tr>
                                        <% }); %>
                                </tbody>
                            </table>
                        </div>
                        <!-- Disabled and active states -->
                    </div>

                </div>
            </div>

            </div>
        </section>
    </main><!-- End #main -->
    <%- include('../master/script'); %>
        <script>
            $(document).ready(function () {
                $('#tableReport').DataTable();
            });
        </script>

</body>

</html>